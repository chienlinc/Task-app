{
    "name": "react-app",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --env mode=dev",
        "build": "webpack --env mode=prod",
        "serve": "live-server dist --entry-file=index.html",
        "deploy": "docker build -t ${DOCKER_USERNAME}/${DOCKER_IMAGE_NAME}:${DOCKER_IMAGE_TAG} . && docker push ${DOCKER_USERNAME}/${DOCKER_IMAGE_NAME}:${DOCKER_IMAGE_TAG}",
        "launch": "docker run --name todo-react -d -p 80:80 ${DOCKER_USERNAME}/${DOCKER_IMAGE_NAME}:${DOCKER_IMAGE_TAG}"
    },
    "sideEffects": false,
    "keywords": [
        "react",
        "webpack",
        "babel",
        "docker",
        "frontend",
        "SPA",
        "ESLint",
        "Prettier",
        "webpack-dev-server",
        "live-server",
        "HTML",
        "CSS",
        "JavaScript",
        "Ant Design",
        "axios",
        "react-router-dom"
    ],
    "author": "Jenny",
    "license": "ISC",
    "description": "A React single-page application powered by Webpack and Docker, featuring Ant Design, Axios, and React Router for efficient and scalable front-end development.",
    "devDependencies": {
        "@babel/cli": "^7.25.9",
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.25.9",
        "@babel/preset-typescript": "^7.26.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "babel-loader": "^9.2.1",
        "compression-webpack-plugin": "^11.1.0",
        "css-loader": "^7.1.2",
        "dotenv-webpack": "^8.1.0",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.37.2",
        "globals": "^15.13.0",
        "html-webpack-plugin": "^5.6.3",
        "live-server": "^1.2.2",
        "prettier": "^3.4.2",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.7.2",
        "webpack": "^5.96.1",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "dependencies": {
        "antd": "^5.22.2",
        "axios": "^1.7.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^7.0.1"
    }
}
